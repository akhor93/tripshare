<div class="container" style="margin: 20px; margin-top: 100px;">
	<table class="table table-bordered table-striped">
		<thead>
			<tr>
				<td style="width:40px">#</td>
				<td style="width:180px">Name</td>
				<td style="width:200px">Location</td>
				<td style="width:180px">Username</td>
				<td>Account Created</td>
			</tr>
		</thead>
		{{#each accts}}
			<tr>
				<td>#</td>
				<td>{{name}}</td>
				<td>{{country}}</td>
				<td>{{username}}</td>
				<td>{{date}}</td>
			</tr>
		{{/each}}
	</table>
</div>